{
  "id": "pong",
  "initial": "waiting",
  "context": {
    "count": 0
  },
  "states": {
    "waiting": {
      "entry": ["onWaiting"],
      "on": {
        "PING": "ponging"
      }
    },
    "ponging": {
      "entry": [
        "incrementCount",
        "sendPong"
      ],
      "on": {
        "PING": "ponging",
        "STOP": "done"
      }
    },
    "done": {
      "entry": ["onDone"],
      "type": "final"
    }
  }
}
